<p id="notice"><%= notice %></p>

<h1>Clientes</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Bi</th>
      <th>Nif</th>
      <th>Utente</th>
      <th>Telefone</th>
      <th>Email</th>
      <th>Seguro</th>
      <th>Numseguro</th>
      <th>Morada</th>
      <th>Observacoes</th>
      <th>Datanascimento</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @clientes.each do |cliente| %>
      <tr>
        <td><%= cliente.nome %></td>
        <td><%= cliente.bi %></td>
        <td><%= cliente.nif %></td>
        <td><%= cliente.utente %></td>
        <td><%= cliente.telefone %></td>
        <td><%= cliente.email %></td>
        <td><%= cliente.seguro %></td>
        <td><%= cliente.numSeguro %></td>
        <td><%= cliente.morada %></td>
        <td><%= cliente.observacoes %></td>
        <td><%= cliente.dataNascimento %></td>
        <td><%= link_to 'Show', cliente %></td>
        <td><%= link_to 'Edit', edit_cliente_path(cliente) %></td>
        <td><%= link_to 'Destroy', cliente, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cliente', new_cliente_path %>
