<p id="notice"><%= notice %></p>

<h1>Dmarcacaos</h1>

<table>
  <thead>
    <tr>
      <th>Cliente</th>
      <th>Start</th>
      <th>End</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dmarcacaos.each do |dmarcacao| %>
      <tr>
        <td><%= dmarcacao.cliente_id %></td>
        <td><%= dmarcacao.start %></td>
        <td><%= dmarcacao.end %></td>
        <td><%= link_to 'Show', dmarcacao %></td>
        <td><%= link_to 'Edit', edit_dmarcacao_path(dmarcacao) %></td>
        <td><%= link_to 'Destroy', dmarcacao, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dmarcacao', new_dmarcacao_path %>
